{
    "workbench.colorCustomizations": {
        "activityBar.background": "#71abed",
        "titleBar.activeBackground": "#622477",
        "titleBar.activeForeground": "#FDFAFE"
    },

    
      "editor.defaultFormatter": "<another formatter>",
      "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "[!s]*/**": true,
        "s[!r]*/**": true,
        "sr[!c]*/**": true,
        "src/[!m]*/**": true,
        "src/m[!o]*/**": true,
        "src/mo[!d]*/**": true,
        "src/mod[!u]*/**": true,
        "src/modu[!l]*/**": true,
        "src/modul[!e]*/**": true,
        "src/module[!s]*/**": true
      },
      
      "code-runner.executorMap": {
        "typescript": "ts-node"
      }
    
      
}

